{
  "manifest_version": 2,
  "version": "1",
  "name": "Changing cloud theme",
  "description": "Changing cloud theme",
  "icons": {
    "48": "images/icon.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Changing cloud theme"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "theme"
  ]
}
